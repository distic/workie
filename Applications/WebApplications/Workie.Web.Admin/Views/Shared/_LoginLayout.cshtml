<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Workie</title>

    <!-- Iconic Fonts -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Bootstrap core CSS -->
    <link href="~/mystic-kernel/light/css/bootstrap.min.css" rel="stylesheet">
    <!-- Mystic styles -->
    <link href="~/mystic-kernel/light/css/style.css" rel="stylesheet">
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="32x32" href="~/mystic-kernel/light/favicon.ico">

</head>
<body class="ms-body ms-primary-theme ms-logged-out">

    <!-- Preloader -->
    <div id="preloader-wrap">
        <div class="spinner spinner-8">
            <div class="ms-circle1 ms-child"></div>
            <div class="ms-circle2 ms-child"></div>
            <div class="ms-circle3 ms-child"></div>
            <div class="ms-circle4 ms-child"></div>
            <div class="ms-circle5 ms-child"></div>
            <div class="ms-circle6 ms-child"></div>
            <div class="ms-circle7 ms-child"></div>
            <div class="ms-circle8 ms-child"></div>
            <div class="ms-circle9 ms-child"></div>
            <div class="ms-circle10 ms-child"></div>
            <div class="ms-circle11 ms-child"></div>
            <div class="ms-circle12 ms-child"></div>
        </div>
    </div>

    <!-- Overlays -->
    <div class="ms-aside-overlay ms-overlay-left ms-toggler" data-target="#ms-side-nav" data-toggle="slideLeft"></div>
    <div class="ms-aside-overlay ms-overlay-right ms-toggler" data-target="#ms-recent-activity" data-toggle="slideRight"></div>

    <!-- Main Content -->
    <main class="body-content">

        <div>
            @RenderBody()
        </div>
    
    </main>

        <!-- SCRIPTS -->
        <!-- Global Required Scripts Start -->
        <script src="~/mystic-kernel/light/js/jquery-3.3.1.min.js"></script>
        <script src="~/mystic-kernel/light/js/popper.min.js"></script>
        <script src="~/mystic-kernel/light/js/bootstrap.min.js"></script>
        <script src="~/mystic-kernel/light/js/perfect-scrollbar.js"></script>
        <script src="~/mystic-kernel/light/js/jquery-ui.min.js"></script>
        <!-- Global Required Scripts End -->
        <!-- Mystic core JavaScript -->
        <script src="~/mystic-kernel/light/js/framework.js"></script>

        <!-- Settings -->
        <script src="~/mystic-kernel/light/js/settings.js"></script>

        @RenderSection("Scripts", required: false)
</body>
</html>
